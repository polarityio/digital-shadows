            <span class="p-key">File Size (bytes): </span>
            <span class="p-value">{{content.entity.fileSizeBytes}}</span>
        </div>
      {{/if}}
      {{#if content.entity.fileLastSeen}}
        <div>
            <span class="p-key">Last Seen: </span>
            <span class="p-value">{{content.entity.fileLastSeen}}</span>
        </div>
      {{/if}}
      {{! CYLANCE_FILE_HASH output parsing}}
      {{#if content.entity.requestedHash}}
        <div>
            <span class="p-key">Requested Hash: </span>
            <span class="p-value">{{content.entity.requestedHash}}</span>
        </div>
      {{/if}}
      {{#if content.entity.fileHashInfo.status}}
