        </div>
      {{/if}}
      {{#if content.entity.fileHashInfo.hashes.md5}}
        <div>
            <span class="p-key">MD5Sum: </span>
            <span class="p-value">{{content.entity.fileHashInfo.hashes.md5}}</span>
        </div>
      {{/if}}
      {{#if content.entity.siteCategories}}
        <h1 class="p-title">{{fa-icon "browser" fixedWidth=true}}Content Categories: </h1>
        {{#each content.entity.siteCategories as |category|}}
          <span class="section-tag">{{category}}</span>
        {{/each}}
      {{/if}}
      {{#if content.snippet}}
        <h1 class="p-title">
          <a
            href="#"
            class="dropdown"
            {{on "click" (action "toggleContentSnippet" index)}}
          >
            <span class="text">
              {{fa-icon "align-justify" fixedWidth=true}}
              Content Snippet
              {{#if content.showContentSnippet}}
                {{fa-icon "angle-up"}}
              {{else}}
                {{fa-icon "angle-down"}}
              {{/if}}
            </span>
          </a>
        </h1>
        {{#if content.showContentSnippet}}
          <div class="p-container">{{content.snippet}}</div>
        {{/if}}
      {{/if}}
  {{/each}}
{{/if}}
