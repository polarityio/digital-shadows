            <span class="p-key">Last Updated: </span>
            <span class="p-value">{{content.entity.lastUpdated}}</span>
        </div>
      {{/if}}
      {{#if content.entity.reputation}}
        <div>
            <span class="p-key">Reputation: </span>
            <span class="p-value">{{content.entity.reputation}}</span>
        </div>
      {{/if}}
      {{! WEBROOT_FILE_HASH output parsing}}
      {{#if (eq content.type "WEBROOT_FILE_HASH")}}
        {{#if content.entity.hash}}
          <div>
              <span class="p-key">Hash: </span>
              <a href="https://portal-digitalshadows.com/search/webrootfilehash/{{content.entity.hash}}" class="p-link">{{content.entity.hash}} {{fa-icon "external-link-square" class="external-link-icon"}}</a>
          </div>
        {{/if}}
      {{/if}}
      {{#if content.entity.timestamp}}
        <div>
            <span class="p-key">Timestamp: </span>
            <span class="p-value">{{content.entity.timestamp}}</span>
        </div>
      {{/if}}
      {{#if content.entity.category}}
        <div>
            <span class="p-key">Category: </span>
            <span class="p-value">{{content.entity.category}}</span>
        </div>
      {{/if}}
      {{#if content.entity.malwareCategory}}
        <div>
            <span class="p-key">Malware Category: </span>
            <span class="p-value">{{content.entity.malwareCategory}}</span>
        </div>
      {{/if}}
      {{#if content.entity.fileSizeBytes}}
